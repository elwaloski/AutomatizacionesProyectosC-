Create procedure [dbo].[Carga_Documentos] @CantidadCargar INT,@Tipo_docu INT,@Holding varchar(12),@RutEmisor varchar(12),@digi varchar(1), @codi_empr INT
 as
begin
DECLARE @var1 varchar(30); 
DECLARE @UltimoFolio INT = 0;
DECLARE @Fecha varchar(30); 
DECLARE @periodo INT = 0;
DECLARE @codi_acec INT = 0; 
DECLARE @codi_acec1 INT = 0;

declare @Existingdate datetime 
Set @Existingdate=GETDATE() 
set @Fecha= (Select  CONVERT(varchar,@Existingdate,23) as [yyyy-mm-dd])

set @periodo=(SELECT FORMAT( getdate(), 'yyyyMM'))


select @codi_acec=codi_acec from dte_acti_econ where codi_acec=620200

if(@codi_acec = 0)
Begin
insert dte_acti_econ values (620200,'Dbnet','Dbnet', null)
END

select @codi_acec1=codi_acec from dte_acec_empr_hold where codi_acec=620200 and codi_emex=''+@Holding+''

if(@codi_acec1 = 0)
Begin
insert dte_acec_empr_hold values(''+@Holding+'',1,620200)
END

IF(@Tipo_docu <> 33 AND  @Tipo_docu <>34 AND  @Tipo_docu <>39 and  @Tipo_docu <>41 and  @Tipo_docu <>43 and  @Tipo_docu <>46 and  @Tipo_docu <> 52 and  @Tipo_docu <>56 and  @Tipo_docu <>61 and  @Tipo_docu <>110 and  @Tipo_docu <>111 and  @Tipo_docu <>112)
        BEGIN
	RETURN   'Tipo de documento No es electronico'
	END
Else 
     BEGIN

IF(@Tipo_docu<>39 and @Tipo_docu<>41)BEGIN

   select top 1 @UltimoFolio=foli_docu from dte_enca_docu_hold where tipo_docu=@Tipo_docu and foli_docu < 1012070  order by FOLI_DOCU desc 
	DECLARE @Total INT = @CantidadCargar+@UltimoFolio;

	

IF(@Tipo_docu=33)	Begin
			
WHILE(@UltimoFolio < @Total ) BEGIN
	set @UltimoFolio+= 1;

insert into dte_enca_docu_hold (codi_emex, codi_empr, tipo_docu, foli_docu, rutt_emis, digi_emis, rutt_rece, digi_rece, mont_neto, fech_emis, esta_docu, mont_tota, nomb_emis, giro_emis, dire_orig, comu_orig, nomb_rece, giro_rece, dire_rece, comu_rece, impu_vaag, tasa_vaag, impr_dest, val1, val2, val3, val4, val5, val6, val7, val8, val9, entr_bien, vent_serv, form_pago, fech_canc, dias_tepa, codi_tepa, mont_brut, moda_pago, fech_venc, peri_desd, peri_hast, nomb_sucu, codi_sucu, ciud_orig, codi_vend, rutt_mand, digi_mand, codi_rece, cont_rece, ciud_rece, dire_post, comu_post, ciud_post, rutt_sofa, digi_sofa, info_tran, rutt_tran, digi_tran, dire_dest, comu_dest, ciud_dest, mont_exen, impu_vanr, cred_es65, gara_enva, mont_nofa, valo_paga, sald_ante, indi_vegd, subt_vese, mont_base, mail_rece, from_paex, tipo_cupa, cuen_pago, banc_pago, glos_pago, iden_adem, iden_reex, naci_ext, nume_book, codi_puem, codi_pude, cant_tara, umed_tara, tota_brut, unid_brut, tota_neto, unid_neto, tota_item, tota_bult, tipo_mone, tipo_camb, neto_otmo, noaf_otmo, faca_otmo, maco_otmo, ivag_otmo, codi_oper, codi_emtr, foli_auto, fech_auto, ivno_otmo, tipo_impr, mont_canc, sald_inso, codi_suad, rutt_chof, digi_chof, nomb_chof, tipo_moom, mont_baco, ivag_prop, ivag_terc, tran_vent, tran_comp, tipo_faes, tipo_turi, feho_ted, indi_nore, moda_vent, clau_expo, tota_clex, viaa_tran, nomb_tran, rutt_citr, digi_citr, nomb_citr, iden_citr, iden_puem, iden_pude, mont_flet, mont_segu, pais_rece, pais_dest,foli_clie, mont_otmo)
values ( ''+@Holding+'', 1, @Tipo_docu, @UltimoFolio,''+@RutEmisor+'', ''+@digi+'', 80043600, '1', 23834, @Fecha, 'ING', 28362, 'KUEHNE + NAGEL LTDA.', 'SERVICIOS DETRANSPORTES Y SERV.', 'AV. APOQUINDO 4501 PISO 14 SANTIAGO CHILE', 'LAS CONDES', ' BASF Chile S.A.', ' FABRICACION PRODUCTOS QUIMICOS INDU', ' Avenida Carrascal3851', 'Santiago', 4528, 19, null, null, null, null, null, null, null, null, null, null, null, null, 2, null, null, null, null, null, '2022-12-12', null, null, null, null, 'SANTIAGO', null, 80043600, '1', ' 1038589601', null, 'Santiago', ' Avenida Carrascal3851', 'Santiago', null, null, null, '', null, null, '', '', '', 0, null, null, null, null, null, null, null, 28362, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null, null)

